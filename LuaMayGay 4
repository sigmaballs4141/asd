-- StarterGui.ProjectReptileV3.Holder.scripts.ScriptHolder.LocalScript
local function C_25()
local script = G2L["25"];
	local SmoothScroll	= require(script.SmoothScroll)
	
	local scrol		= script.Parent
	
	SmoothScroll.Enable(scrol)
	
	local prevMin = 0.5
	local prevMax = 400
	
	local Player = game.Players.LocalPlayer
	Cam = workspace.CurrentCamera
	Head = Player.Character.Head
	script.Parent.MouseEnter:connect(function()
	    local camDist = (Cam.CoordinateFrame.p - Head.Position).magnitude
	    prevMin = Player.CameraMinZoomDistance
	    prevMax = Player.CameraMaxZoomDistance
	    Player.CameraMinZoomDistance = camDist --Lock the zoom to whatever distance the camera is from the head
	    Player.CameraMaxZoomDistance = camDist
	end)
	
	script.Parent.MouseLeave:connect(function()
	    Player.CameraMinZoomDistance = prevMin --revert the zoom back to what it was before
	    Player.CameraMaxZoomDistance = prevMax
	end)
end;
task.spawn(C_25);
