Closure = function()
    local script = G2L["29"];function CircleClick(d,b,a)coroutine.resume(coroutine.create(function()d.ClipsDescendants=true local c=script:WaitForChild("Circle"):Clone()c.Parent=d local b=b-c.AbsolutePosition.X local a=a-c.AbsolutePosition.Y c.Position=UDim2.new(0,b,0,a)local a=0 if d.AbsoluteSize.X>d.AbsoluteSize.Y then a=d.AbsoluteSize.X*1.5 elseif d.AbsoluteSize.X<d.AbsoluteSize.Y then a=d.AbsoluteSize.Y*1.5 elseif d.AbsoluteSize.X==d.AbsoluteSize.Y then a=d.AbsoluteSize.X*1.5 end local b=0.5 c:TweenSizeAndPosition(UDim2.new(0,a,0,a),UDim2.new(0.5,-a/2,0.5,-a/2),"Out","Quad",b,false,nil)for a=1,10 do c.ImageTransparency=c.ImageTransparency+0.01 wait(b/10)end c:Destroy()end))end return CircleClick
