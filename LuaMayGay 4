-- StarterGui.ProjectReptileV3.Holder.mainmenu.Scripts.LocalScript
local function C_13()
local script = G2L["13"];
	local function findPlayer(name)
		name = name:lower()
		if name == 'me' then
			return game:GetService'Players'.LocalPlayer
		end
		for i,v in pairs(game:GetService'Players':GetPlayers()) do
			if v.Name:lower():find(name) == 1 then
				return v
			end
		end
	end
	
	if(_G.TweenDB==nil)then
		_G.TweenDB=false
	end
	
	local currpage = script.Parent.Parent
	local nextpage = script.Parent.Parent.Parent:WaitForChild"scripts"
	script.Parent.MouseButton1Click:connect(function()
		if(_G.TweenDB)then return end
		_G.TweenDB=true
		nextpage.Visible=true
		currpage.Visible=true
		nextpage.Position = UDim2.new(1, 0,-0.001, 0)
		currpage:TweenPosition(UDim2.new(-1, 0,0,0),Enum.EasingDirection.In,Enum.EasingStyle.Back,.6,true)
		wait(.4)
		nextpage:TweenPosition(UDim2.new(0,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Back,.5,true)
		wait(.5)
		_G.TweenDB=false
	end)
end;
task.spawn(C_13);
