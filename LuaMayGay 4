local script = G2L["4d"];
	local function findPlayer(name)
		name = name:lower()
		if name == 'me' then
			return game:GetService'Players'.LocalPlayer
		end
		for i,v in pairs(game:GetService'Players':GetPlayers()) do
			if v.Name:lower():find(name) == 1 then
				return v
			end
		end
	end
	
	if(_G.TweenDB==nil)then
		_G.TweenDB=false
	end
	
	local currpage = script.Parent.Parent
	local nextpage = script.Parent.Parent.Parent:WaitForChild"mainmenu"
	script.Parent.MouseButton1Click:connect(function()
		if(_G.TweenDB)then return end
		_G.TweenDB=true
		nextpage.Visible=true
		currpage.Visible=true
		nextpage.Position = UDim2.new(-1, 0,-0.001, 0)
		currpage:TweenPosition(UDim2.new(1, 0,0,0),Enum.EasingDirection.In,Enum.EasingStyle.Back,.6,true)
		wait(.4)
		nextpage:TweenPosition(UDim2.new(0,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Back,.5,true)
		wait(.5)
		_G.TweenDB=false
	end)
end;
task.spawn(C_4d);
-- StarterGui.ProjectReptileV3.Holder.discord.TextButton.LocalScript
local function C_53()
local script = G2L["53"];
	local function findPlayer(name)
		name = name:lower()
		if name == 'me' then
			return game:GetService'Players'.LocalPlayer
		end
		for i,v in pairs(game:GetService'Players':GetPlayers()) do
			if v.Name:lower():find(name) == 1 then
				return v
			end
		end
	end
	
	if(_G.TweenDB==nil)then
		_G.TweenDB=false
	end
	
	local currpage = script.Parent.Parent
	local nextpage = script.Parent.Parent.Parent:WaitForChild"mainmenu"
	script.Parent.MouseButton1Click:connect(function()
		if(_G.TweenDB)then return end
		_G.TweenDB=true
		nextpage.Visible=true
		currpage.Visible=true
		nextpage.Position = UDim2.new(-1, 0,-0.001, 0)
		currpage:TweenPosition(UDim2.new(1, 0,0,0),Enum.EasingDirection.In,Enum.EasingStyle.Back,.6,true)
		wait(.4)
		nextpage:TweenPosition(UDim2.new(0,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Back,.5,true)
		wait(.5)
		_G.TweenDB=false
	end)
end;
task.spawn(C_53);
-- StarterGui.ProjectReptileV3.Holder.login.TextBox.effectR.click
local function C_59()
local script = G2L["59"];
	moduleLocation=script.Parent guiObject=script.Parent.Parent m=game.Players.LocalPlayer:GetMouse()RippleEffect=require(moduleLocation)guiObject.MouseButton1Down:connect(function()RippleEffect(guiObject,m.X,m.Y)end)
end;
task.spawn(C_59);
-- StarterGui.ProjectReptileV3.Holder.login.TextButton.LocalScript
local function C_5b()
local script = G2L["5b"];
	local code = script.Parent.Parent.Code
	local textbox = script.Parent.Parent.TextBox
	
	
	script.Parent.MouseButton1Click:connect(function()
		if textbox.Text == code.Value then
			script.Parent.Text = 'Welcome!'
			wait(1)
			script.Parent.Parent.Visible = false
			script.Parent.Parent.mainmenu.Visible = true
	else
			game:GetService('Players').LocalPlayer:Kick('Bye, skid kid!')
		end
	end)
end;
task.spawn(C_5b);
